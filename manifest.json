{
    "name": "Example app",
    "id": "example",
    "packaging_format": 1,
    "description": {
        "en": "Automatic TV shows downloader",
        "fr": "Téléchargement automatisé de séries TV"
    },
    "version": "0.5.12~ynh1",
    "url": "https://example.com",
    "upstream": {
        "license": "GPL-3.0 License",
        "website": "https://pymedusa.com/",
        "code": "https://github.com/pymedusa/Medusa"
    },
    "license": "GPL-3.0 License",
    "maintainer": {
        "name": ""
    },
    "requirements": {
        "yunohost": ">= 4.2.4"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/medusa",
                "default": "/medusa"
            },
            {
                "name": "admin",
                "type": "user",
                "example": "johndoe"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
